{
    "name" : "grossum/faq-bundle",
    "description" : "This bundle added faq entity with fields: enabled, question, answer, position, createdAt and updatedAt",
    "type" : "symfony-bundle",
    "authors" : [{
        "name" : "Alexey Chernoivan",
        "email" : "achernoivan@grossum.com"
    }],
    "keywords" : [
        "faq bundle"
    ],
    "license" : [
        "MIT"
    ],
    "require": {
        "php": ">=5.5.2",
        "symfony/framework-bundle": "~2.1",
        "sonata-project/core-bundle": "~2.2",
        "sonata-project/admin-bundle": "~2.2",
        "knplabs/knp-menu-bundle": "~2.0",
        "grossum/core-bundle": "0.1.*@dev"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd" : "@stable",
        "sebastian/phpcpd": "*"
    },
    "autoload" : {
        "psr-0" : {
            "Grossum\\FaqBundle" : ""
        }
    },
    "target-dir" : "Grossum/FaqBundle",
    "extra" : {
    "branch-alias" : {
            "dev-master" : "0.1-dev"
        }
    }
}
